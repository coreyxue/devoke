<!DOCTYPE html>
<html>
<head>
  <title>Devoke</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.12.0.min.js"></script>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="header">
		<img class="icon" src="../../assets/icon.jpg">
		<p class="header_verse"></p>
	</div>
	<div class="sidebar">
		<div class="sideButton">
			<%= image_tag 'clipboard.png', :class => 'icon'%>
		</div>
		<div class="sideButton">
			<%= image_tag 'goal.png', :class => 'icon'%> 
		</div>
		<div class="sideButton">
			<%= image_tag 'arrow.png', :class => 'icon'%>
		</div>
		<div class="sideButton">
			<%= image_tag 'book.png', :class => 'icon'%>
		</div>
		<div class="sideButton">
			<%= image_tag 'pocket.png', :class => 'icon'%>
		</div>
	</div>

<%= yield %>

	<div class="footer"></div>
</body>
</html>
